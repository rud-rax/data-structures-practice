    "def solve(n) :\n",
    "    c = 0 \n",
    "    while n > 0 : \n",
    "\n",
    "        for i in range(100) :\n",
    "            if n <= (i**2) : \n",
    "                n -= (i - 1 )**2\n",
    "                c+=1\n",
    "\n",
    "    return c"
   ]
  },
